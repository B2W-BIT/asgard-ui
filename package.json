{
  "name": "marathon-ui",
  "version": "0.9.0",
  "description": "The web UI for Mesosphere's Marathon.",
  "main": "dist/main.js",
  "keywords": [
    "marathon",
    "mesos"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mesosphere/marathon-ui.git"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "classnames": "^2.1.2",
    "flux": "^2.0.3",
    "jquery": "^2.1.3",
    "lazy.js": "^0.4.0",
    "moment": "^2.9.0",
    "mousetrap": "^1.4.6",
    "oboe": "^2.1.2",
    "react": "^0.13.2",
    "react-tools": "^0.13.2",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "broccoli": "^0.16.3",
    "broccoli-autoprefixer": "^3.0.0",
    "broccoli-clean-css": "^0.3.0",
    "broccoli-env": "^0.0.1",
    "broccoli-funnel": "^0.2.3",
    "broccoli-less-single": "^0.4.0",
    "broccoli-lint-eslint": "^1.0.1",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-uglify-js": "^0.1.3",
    "broccoli-webpack": "^0.1.1",
    "chai": "^3.0.0",
    "chalk": "^0.5.1",
    "cheerio": "^0.18.0",
    "envify": "^3.4.0",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.1.0",
    "npm-shrinkwrap": "^5.4.0",
    "sinon": "^1.12.2",
    "source-map-loader": "^0.1.5",
    "transform-loader": "^0.2.2"
  },
  "scripts": {
    "build": "broccoli build dist",
    "clean": "rm -rf dist tmp",
    "dist": "export BROCCOLI_ENV=\"production\" && npm run clean && npm run test && npm run build",
    "test": "mocha --ui exports --reporter spec --compilers jsx:src/test/helpers/compiler.js src/test/helpers/object-assign-polyfill.js src/test/**/*.test.* src/test/*.test.*",
    "serve": "export BROCCOLI_WARN_READ_API=y BROCCOLI_ENV=\"development\" DISABLE_SOURCE_MAP=false && broccoli serve",
    "shrinkwrap": "npm-shrinkwrap --dev"
  }
}
