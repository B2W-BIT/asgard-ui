{
  "name": "marathon-ui",
  "version": "0.9.0",
  "description": "The web UI for Mesosphere's Marathon.",
  "main": "dist/main.js",
  "keywords": [
    "marathon",
    "mesos"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mesosphere/marathon-ui.git"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "classnames": "^2.1.2",
    "flux": "^2.0.3",
    "jquery": "^2.1.3",
    "lazy.js": "^0.4.0",
    "moment": "^2.9.0",
    "mousetrap": "^1.4.6",
    "oboe": "^2.1.2",
    "react": "^0.13.2",
    "react-tools": "^0.13.2",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "chai": "^3.0.0",
    "chalk": "^0.5.1",
    "cheerio": "^0.18.0",
    "envify": "^3.4.0",
    "eslint-plugin-react": "^2.5.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-changed": "^1.2.1",
    "gulp-connect": "^2.2.0",
    "gulp-copy": "0.0.2",
    "gulp-eslint": "^0.14.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.1.6",
    "gulp-mocha": "^2.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.5",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.1.0",
    "npm-shrinkwrap": "^5.4.0",
    "node-libs-browser": "^0.5.2",
    "sinon": "^1.12.2",
    "source-map-loader": "^0.1.5",
    "transform-loader": "^0.2.2",
    "webpack": "^1.9.11"
  },
  "scripts": {
    "build": "export GULP_ENV=\"development\" && gulp",
    "clean": "rm -rf dist",
    "dist": "export GULP_ENV=\"production\" && npm run clean && npm run test && npm run build",
    "shrinkwrap": "npm-shrinkwrap --dev",
    "test": "mocha --ui exports --reporter spec --compilers jsx:src/test/helpers/compiler.js src/test/helpers/object-assign-polyfill.js src/test/**/*.test.* src/test/*.test.*",
    "serve": "export GULP_ENV=\"development\" DISABLE_SOURCE_MAP=false && gulp serve",
    "watch": "export GULP_ENV=\"development\" && gulp watch"
  }
}
